  "name": "prakta",
  "version": "1.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint --fix",
    "format": "prettier --write src",
    "ts": "tsc --noEmit",
    "db:push": "dotenv -c -- prisma db push",
    "db:seed": "dotenv -c -- node prisma/seed.mjs",
    "db:studio": "dotenv -c -- prisma studio",
    "postinstall": "prisma generate",
    "prepare": "husky install"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": [
      "eslint --fix"
